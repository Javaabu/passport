{
    "name": "javaabu/passport",
    "description": "laravel/passport with modifications",
    "homepage": "https://github.com/Javaabu/passport",
    "license": "MIT",
    "authors": [
        {
            "name": "Athfan Khaleel",
            "email": "athfan@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "laravel/passport": "^9.0 || ^10.0 || ^11.10",
        "javaabu/helpers": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5",
        "spatie/laravel-query-builder": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Passport\\PassportServiceProvider"
            ],
            "dont-discover": [
                "laravel/passport"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
